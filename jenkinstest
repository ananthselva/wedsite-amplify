pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
stage('Build') {
            steps {
                // Build your project
                // Example: Maven build
                echo 'build'
            }
        }
  stage('SonarQube Analysis') {
steps {
                script {
                    def scannerHome = tool 'sonarqubescanner'
                    withSonarQubeEnv('http://18.171.52.34:9000/') {
                        sh "${sonarqubescanner}/bin/sonar-scanner"
    }
   }
  }
}
}
}
